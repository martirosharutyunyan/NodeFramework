{
  "name": "NodeFramework",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --config nodemon.json",
    "server": "npm run build && node fastify.js",
    "build": "tsc && node createServer.js",
    "migration:create": "node ./node_modules/typeorm/cli.js migration:create -f ormconfig -d application/migrations -n",
    "migration:generate": "node ./node_modules/typeorm/cli.js migration:generate -f ormconfig -d application/migrations -n",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -f ormconfig",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -f ormconfig"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.20.2",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@types/pg": "^8.6.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
